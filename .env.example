# Example environment variables for Cafe Forecasting Platform
# Copy this file to .env and fill in your actual secrets/keys

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# Google Search API
GOOGLE_SEARCH_API_KEY=your-google-search-api-key-here
GOOGLE_SEARCH_CX=your-google-search-cx-here

# Metaculus API
METACULUS_API_URL=https://www.metaculus.com/api/
METACULUS_API_KEY=your-metaculus-api-key-here

# vLLM Model
VLLM_MODEL_PATH=/path/to/your/vllm/model
